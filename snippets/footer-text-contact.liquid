        <p style="font-size: 14px;">{{block.settings.address}}</p>
      </div>
    </div>
  </div>
{% style %}
    .text-contact-container .items {
        display:flex;
        margin-bottom:10px
    }
    .text-contact-container .custom-icont {
        color:#999;
        height:20px
    }
    .text-contact-container p {
        color:#666;
        font-size:14px;
        margin-left:10px;
      
    }
{% endstyle %}